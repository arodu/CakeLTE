{
  "name": "arodu/cakelte",
  "description": "CakeLTE: AdminLTE plugin for CakePHP 4.x",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "almasaeed2010/adminlte": "~3.2",
    "cakephp/cakephp": "^4.0",
    "friendsofcake/bootstrap-ui": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "CakeLte\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CakeLte\\Test\\": "tests/",
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "bin/cake cakelte install"
    ]
  }
}
